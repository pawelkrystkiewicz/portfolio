{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "baseUrl": ".",	"rootDir": ".",
    "paths": {
      "@components/*": ["./components/*"],
      "@components": ["./components"],
      "@layouts/*": ["./layouts/*"],
      "@layouts": ["./layouts"],
      "@pages/*": ["./pages/*"],
      "@pages": ["./pages"],
      "@public/*": ["./public/*"],
      "@public": ["./public"],
      "@routes/*": ["./routes/*"],
      "@routes": ["./routes"],
      "@static/*": ["./static/*"],
      "@static": ["./static"],
      "@store/*": ["./store/*"],
      "@store": ["./store"],
    },
    "typeRoots": [
      "./node_modules/@types"
    ],
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
